{
  "extends": "bliss",
  "rules": {
    "fp/no-this": "off",
    "fp/no-mutation": "off",
    "fp/no-let": "off",
    "fp/no-throw": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "flowtype/boolean-style": "off",
    "flowtype-errors/show-errors": "warn"
  }
}
